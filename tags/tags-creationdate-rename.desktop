[Desktop Entry]
Type=Action
Icon=insert-text
Name[C]=Rename to Date_Time-Country-City
Tooltip[C]=Rename file according to its Creation date, Country & City 
MimeType=image/all;
Capabilities=Writable
Profiles=rename;

[X-Action-Profile rename]
TryExec=/usr/local/bin/tags-creationdate
Exec=/usr/local/bin/tags-creationdate --rename --country --city %F
